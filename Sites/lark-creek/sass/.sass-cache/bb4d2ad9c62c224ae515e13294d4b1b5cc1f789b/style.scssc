        height: 38px;
        line-height: 38px;
        padding: 0 16px;
        display: block;
        &:hover {
          color: #A9A39F;
        }
      }
      &.current_page_item a, &.current-page-ancestor a {
        color: #A9A39F;
      }
      &.invert {
        background: #fff;
        a {
          color: $brown;
        }
      }
