:@template"=/*!
 * Theme Name:  Lark Creek Theme
 * Description: Theme for the Lark Creek family of websites, based on SOEKANTheme
 * Author:      Lion Associates
 * Author URI:  http://lionassociates.com/
 * Version:     1.0
 */

/* 0. Imports, Variables, & Mixins
 * ========================================================================= */

// -------------------------------> Imports <------------------------------- */

// Normalize.css: http://necolas.github.io/normalize.css/
@import "normalize";

// Font Awesome: http://fortawesome.github.io/Font-Awesome/
@import "font-awesome/font-awesome";

// Soekan styles (Old Theme)
@import "soekan/soekan_style";
//@import "soekan/soekan_screen";
@import "soekan/maximage";


// ------------------------------> Variables <------------------------------ */
// SASS Variables go here: colors, sizes, etc.

// Turn the grid system on/off
$include-grid: true;

// Content sizing
$content-width: 1140px;
$content-padding: 30px;

// Breakpoints (feel free to use more than these)
$large-desktop: 1140px / 16px * 1em;
$small-desktop: 960px / 16px * 1em;
$ipad-landscape: 1024px / 16px * 1em;
$ipad-portrait: 768px / 16px * 1em;
$iphone-landscape: 480px / 16px * 1em;
$iphone-portrait: 320px / 16px * 1em;

// Colors
$site-bg: #eee;
$dark-text: #333;
$light-text: #ccc;

$link-normal: #ffffff;
$link-hover: darken( $link-normal, 10% );
$brown: #544F49;

// Text & Fonts
$base-font-size: 16px;
$large-font-size: 20px;
$small-font-size: 12px;
$base-line-height: 1.5;


// -------------------------------> Mixins <-------------------------------- */

// Helper Functions
@mixin clearfix {
  &:after {
    content: '';
    display: table;
    clear: both;
  }
}

@mixin prefix( $property, $shorthand ) {
  -webkit-#{ $property }: #{ $shorthand };
     -moz-#{ $property }: #{ $shorthand };
      -ms-#{ $property }: #{ $shorthand };
       -o-#{ $property }: #{ $shorthand };
          #{ $property }: #{ $shorthand };
}

@mixin box-sizing( $value: border-box ) {
  @include prefix( box-sizing, $value );
}

// Styles
@mixin vertical-gradient( $top, $bottom ) {
  background: $top;
  background: -webkit-gradient( linear, left top, left bottom, 
                color-stop( 0%, $top ), 
                color-stop( 100%, $bottom )
              );
  background: -webkit-linear-gradient( top, $top 0%, $bottom 100% );
  background:    -moz-linear-gradient( top, $top 0%, $bottom 100% );
  background:     -ms-linear-gradient( top, $top 0%, $bottom 100% );
  background:      -o-linear-gradient( top, $top 0%, $bottom 100% );
  background:         linear-gradient( to bottom, $top 0%, $bottom 100% );
      filter: progid:DXImageTransform.Microsoft.gradient(
                startColorstr='#{ie_hex_str( $top )}',
                endColorstr='#{ie_hex_str( $bottom )}', 
                GradientType=0 
              );
}

@mixin horizontal-gradient( $left, $right ) {
  background: $left;
  background: -webkit-gradient( linear, left top, right top,
                color-stop( 0%, $left ),
                color-stop( 100%, $right ) 
              );
  background: -webkit-linear-gradient( left, $left 0%, $right 100% );
  background:    -moz-linear-gradient( left, $left 0%, $right 100% );
  background:     -ms-linear-gradient( left, $left 0%, $right 100% );
  background:      -o-linear-gradient( left, $left 0%, $right 100% );
  background:         linear-gradient( to right, $left 0%, $right 100% );
      filter: progid:DXImageTransform.Microsoft.gradient( 
                startColorstr='#{ie_hex_str( $left )}', 
                endColorstr='#{ie_hex_str( $right )}',
                GradientType=1 
              );
}

@mixin text-overflow {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

@mixin transparent-bg( $color, $opacity ) {
  $rgba: rgba( $color, $opacity );
  background: $rgba;
  filter:progid:DXImageTransform.Microsoft.gradient(
    startColorstr='#{ie_hex_str( $rgba )}',
    endColorstr='#{ie_hex_str( $rgba )}'
  ); 
}



/* 1. Base
 * ========================================================================= */
// Base theme styling goes here: only elements (e.g. "body"); no classes or IDs
// Headings (h1-h6), links, inputs, etc.

body {
  background-color: $site-bg;
  color: $dark-text;
  font-size: $base-font-size;
  line-height: $base-line-height;
}
@media only screen and ( max-width: 47em ) {
  body {
    min-width: 320px;
  }
}


a {
  color: $link-normal;
  text-decoration: none;
  @include prefix( transition, color 0.15s ease-out );

  &:hover {
    color: $link-hover;
  }
}

nav {
  float: right;
  ul {
    list-style: none;
    padding: 0;
    font-size: 0.875em;
    font-weight: bold;
    height: 38px;
    margin-right: 34px;
    text-transform: uppercase;
    li {
      float: left;
      a {
        color: #fff;
        height: 38px;
        line-height: 38px;
        padding: 0 16px;
        display: block;
        &:hover {
          color: #A9A39F;
        }
      }
      &.current_page_item a, &.current-page-ancestor a {
        color: #A9A39F;
      }
      &.invert {
        background: #fff;
        a {
          color: $brown;
        }
      }
    }
  }
}

nav#footer-menu {
  ul {
    height: 30px;
    float: left;
    margin-right: 10px;
    li a {
      height: 30px;
      line-height: 30px;
      display: block;
      padding: 0 10px;
    }
  }
  .social {
    a {
      line-height: 30px;
      width: 20px;
      text-align: center;
      display: block;
      float: left;
    }
  }
}

.subNav {
  h4 {
    font-size: 16px;
    margin-bottom: 0.5em;
    text-transform: uppercase;
  }
  ul {
    list-style: none;
    margin: 20px 0 0;
    padding: 0;
    li {
      margin-bottom: 0.6em;
      a {
        font-size: 0.875em;
        line-height: 1.1em;
        display: block;
      }
      span {
        font-size: 13px;
        color: #eee;
      }
    }
  }
}

#mobile-button {
  height: 38px;
  width: 44px;
  position: absolute;
  top: 0;
  right: 10px;
  z-index: 999;
  text-align: right;
  cursor: pointer;
  display: none;
  i {
    font-size: 2em;
    color: #fff;
    line-height: 40px;
  }
}

#mobile-nav {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  background: $brown;
  width: 240px;
  overflow: auto;
  z-index: 0;
  display: none;
  ul {
    list-style: none;
    padding: 0;
    margin: 0;
    width: 100%;
    li {
      text-align: right;
      border-bottom: 1px solid #78746f;
      a {
        padding: 0 20px;
        line-height: 40px;
        display: block;
        font-size: 14px;
      }
    }
  }
}

#master-wrap {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  z-index: 1;
  -webkit-box-shadow: 0 0 5px 2px rgba(0,0,0,0.5);
  box-shadow: 0 0 5px 2px rgba(0,0,0,0.5);
}
#fullscreen {
  background: #eee;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  z-index: 0;
  .slide {
    min-width: 100%;
    min-height: 100%;
  }
}


@media only screen and ( max-width: 58em ) {
  #mobile-button, #mobile-nav {
    display: block;
  }
  #main-menu .menu, #footer-menu .menu {
    display: none;
  }
  .social {
    margin-right: 2px;
  }
  #site-footer #site-info {
    margin-left: 10px;
  }
  #arrow_right {
    right: 10px;
  }
  .logo, .container-left, #arrow_left {
    left: 10px;
  }
  .container-right {
    right: 10px;
    width: 50%;
  }
}


/* 2. Structural Styles
 * ========================================================================= */
// Styles that define the structure of the theme (e.g. content width, 
// columns, etc.)

.content-width {
  max-width: $content-width - $content-padding * 2;
  padding-left: $content-padding;
  padding-right: $content-padding;
  margin-left: auto;
  margin-right: auto;
}

.content-left {
  color: #fff;
  font-size: 1.2em;
  .promo-link {
    margin-top: 20px;
    display: block;
  }
  .subNav p {
    font-size: 14px;
  }
}

#site-info {
  color: #fff;
  margin: 0 0 0 40px;
  float: left;
  font-size: 0.8em;
  line-height: 30px;
}

.home .container-left {
  background-color: rgba(255, 255, 255, 0.7);
}
.home .content-left {
  color: #544F49;
}
.home .content-left a {
  color: #544F49;
  font-size: 0.9em;
  text-decoration: underline;
}

.content-left h1 {
    color: #544F49;
    font-size: 1.33333em;
    line-height: 1.06667em;
    margin: 0 0 0.5em;
}
.content-left h2 {
    font-size: 1em;
    line-height: 1.1em;
    font-weight: normal;
    margin-bottom: 20px;
}

.events .container-right, .blog .container-right {
  width: 55%;
}
.event-thumb {
  margin-bottom: 20px;
  display: block;
}
.blogPost {
    @include box-sizing;
    border-top: 1px solid #FFFFFF;
    margin: 1.5em 0;
    padding: 1.5em 0 0;
    clear: both;
    .left {
      margin: 10px 20px 20px 0;
      float: left;
      display: block;
    }
    .right {
      margin: 10px 0 20px 20px;
      float: right;
      display: block;
    }
}

.partner-logo {
  max-width: 200px;
  max-height: 140px;
  width: auto;
  height: auto;
  display: block;
  margin: 0 auto;
}

.partners .content-right h3 {
  margin-bottom: 10px;
  a {
      text-decoration: none;
    display: block;
  } 
}

.headshot {
  max-width: 175px;
  height: auto;
  float: right;
  display: block;
  margin: 20px 0 10px 20px;
  border: 1px solid #545454;
}

.logo {
  img {
    width: 216px;
    height: auto;
    display: block;
  }
}

.menu .container-right {
  width: 66%;
  max-width: 960px;
}
@media only screen and ( max-width: 44em ) { 
  .menu .container-right {
    width: auto;
    top: 8em;
    left: 10px;
    right: 10px;
  }
}

.blog .content-left {
  font-size: 0.8em;
}


@media only screen and ( max-width: 30em ) {
  #master-wrap {
    height: auto;
    min-height: 100%;
  }
  .logo {
    left: auto;
    margin: 0 auto 20px;
    position: relative;
    width: 280px;
    img {
      width: 100%;
      display: block;
    }
  }
  .container-left, .container-right {
    width: 280px;
    margin: 0 auto 20px;
    left: auto;
    right: auto;
    top: auto;
    bottom: auto;
    position: relative;
    clear: both;
  }
  .container-right {
    margin-bottom: 80px;
  }
  .menu .container-right {
    margin-right: 10px;
    margin-left: 10px;
    right: auto;
    left: auto;
    top: 0;
  }
  .header {
    position: relative;
  }
}

#open-table-widget {
  position: absolute;
  top: 70px;
  right: 40px;
}


/* -------------------------------> Columns <------------------------------- */
// To use the grid system, the wrapper element receives the class "columns" as
// well as the size (e.g. "two"). Each column receives the class "column".
// Example:
// 
// <div class="columns three">
//   <div class="column">Column 1 Content</div>
//   <div class="column">Column 2 Content</div>
//   <div class="column">Column 3 Content</div>
// </div>
// 
// Reverse the column order by adding the class "reverse" to the parent
// container element, e.g.:
// 
// <div class="columns two reverse">
//   [...]
// </div>
// 
// You can also increase the size of a given column by adding the class "x#" to
// the column element, where "#" is the width multiplier. Remove one column
// for each width multiple. For example, to acheive a two-thirds layout, you
// would have the structure:
// 
// <div class="columns three">
//   <div class="column">Column 1 Content</div>
//   <div class="column x2">Column 2 Content</div>
// </div>
// 
// Finally, you can "push" a column to the left by adding the class "push-#",
// where "#" is the number of column-widths to offset the column. Again, this
// will take the place of the number of columns you are pushing by, so remove
// the appropriate number of column elements. The structure:
// 
// <div class="columns four">
//   <div class="column">Column 1 Content</div>
//   <div class="column push-2">Column 2 Content</div>
// </div>
// 
// will create a two column layout with the second column offset to the far
// left of the screen. Adding the "reverse" class will alternatively push 
// the columns to the right by the given amount.

@if $include-grid {
  // Base Styles
  .columns {
    @include clearfix;

    &.reverse {
      > .column {
        float: right;
      }
    }

    // Two columns
    &.two {  
      > .column {
        width: 50%;
      }

      > .push-1 {
        margin-left: 33.333%;
      }

      &.reverse > .push-1 {
        margin-right: 33.333%;
        margin-left: 0;
      }
    }

    // Three Columns
    &.three {
      > .column {
        width: 33.333%;
      }

      > .x2 {
        width: 66.667%
      }

      > .push-1 {
        margin-left: 33.333%;
      }

      > .push-2 {
        margin-left: 66.667%;
      }

      &.reverse {
        > .push-1 {
          margin-right: 33.333%;
          margin-left: 0;
        }

        > .push-2 {
          margin-right: 66.667%;
          margin-left: 0;
        }
      }
    }

    // Four Columns
    &.four {
      > .column {
        width: 25%;
      }

      > .x2 {
        width: 50%;
      }

      > .x3 {
        width: 75%;
      }

      > .push-1 {
        margin-left: 25%;
      }

      > .push-2 {
        margin-left: 50%;
      }

      > .push-3 {
        margin-left: 75%;
      }

      &.reverse {
        > .push-1 {
          margin-right: 25%;
          margin-left: 0;
        }

        > .push-2 {
          margin-right: 50%;
          margin-left: 0;
        }

        > .push-3 {
          margin-right: 75%;
          margin-left: 0;
        }
      }
    }

    &.five {
      > .column {
        width: 20%;
      }

      > .x2 {
        width: 40%;
      }

      > .x3 {
        width: 60%;
      }

      > .x4 {
        width: 80%;
      }

      > .push-1 {
        margin-left: 20%;
      }

      > .push-2 {
        margin-left: 40%;
      }

      > .push-3 {
        margin-left: 60%;
      }

      > .push-4 {
        margin-left: 80%;
      }      

      &.reverse {
        > .push-1 {
          margin-right: 20%;
          margin-left: 0;
        }

        > .push-2 {
          margin-right: 40%;
          margin-left: 0;
        }

        > .push-3 {
          margin-right: 60%;
          margin-left: 0;
        }

        > .push-4 {
          margin-right: 80%;
          margin-left: 0;
        }
      }
    }
  }

  .column {
    float: left;
    @include box-sizing;
  }
}





/* 3. Element Styles
 * ========================================================================= */
// Main styles for the site: resuable styles (e.g. ".button") and module styles

/* --------------------------------> State <-------------------------------- */
.is-hidden {
  display: none;
}


/* -------------------------------> General <------------------------------- */
.alignleft {
  display: inline;
  float: left;
  margin-right: 20px;
}

.alignright {
  display: inline;
  float: right;
  margin-left: 20px;
}

.aligncenter {
  clear: both;
  display: block;
  margin-left: auto;
  margin-right: auto;
}


/* -------------------------------> Modules <------------------------------- */
// Media queries should follow each module in the form:
// 
// .module-name {
//   width: 50%;
// }
// 
// @media only screen and ( max-width: $small-desktop ) {
//   .module-name {
//     width: 75%;
//   }
// }
// 
// Rather than focusing on specific breakpoints (although common breakpoints 
// are provided), add media queries as necessary to best accomodate the layout
// of the content itself. All media queries should be given in ems rather than
// pixels. The SASS function emCalc is useful for converting pixels to ems:
// 
// @media only screen and ( max-width: emCalc( 1152px ) ) {
//   [...]
// }
